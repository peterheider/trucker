{
    "name": "imi/trucker",
    "description": "A simple active resource implementation written in PHP to consume any REST API",
    "keywords": [
        "trucker",
        "activeresource",
        "active resource"
    ],
    "type": "library",
    "homepage": "http://indatus.com/",
    "license": "MIT",
    "authors": [
        {
            "name": "Brian Webb",
            "homepage": "http://indatus.com/",
            "email": "bwebb@indatus.com",
            "role": "Developer"
        }
    ],
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/brianwebb01/filesystem"
        },
        {
            "type": "vcs",
            "url": "https://github.com/brianwebb01/hot-phpunit-runner"
        }
    ],
    "require": {
        "php": ">=7.0.0",
        "illuminate/support": "^5.0",
        "illuminate/config": "^5.0",
        "illuminate/container": "^5.0",
        "doctrine/inflector": "^1.1",
        "patchwork/utf8": "1.1.*",
        "guzzlehttp/guzzle": "^6.2@dev",
        "laravel/framework": "5.7.*"
    },
    "require-dev": {
        "mockery/mockery": "~1.0",
        "phpunit/phpunit": "~6.0",
        "satooshi/php-coveralls": "dev-master",
        "hot/phpunit-runner": "dev-master"
    },
    "autoload": {
        "psr-0": {
            "Trucker": "src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.1-dev"
        }
    },
    "minimum-stability": "dev"
}
